bool isSubsequence(string s, string t) {
        int n1=t.size();
        int n2=s.size();
        int i=0;
        int j=0;
        while(j<n2&&i<n1)  //比较运用双指针，不需要用具体的如s[0],s[1]去比较。
,        {
            if(s[j]==t[i])
          {
            j++;
          }
          i++;
        }
        return j==n2;  ///包含了两个字符串都为空的情况，直接省略一层if和else的过程
    }
};
