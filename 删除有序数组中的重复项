int removeDuplicates(vector<int>& nums) {
        int n=nums.size();
        int left=1;
        for(int right=1;right<n;right++)
        {
            for(int j=right-1;j>=0;j--)
            {
                if(nums[j]==nums[right])
                {
                   left--;
                   break;
                }
            }
            nums[left]=nums[right];
            left++;
        }
        return left;
    }
